{
  "name": "build-lambda-layer",
  "version": "0.0.2",
  "description": "Build node awesome lambda layers with controls ðŸ•¹",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "build-lambda-layer": "dist/index.cjs",
    "build-layer": "dist/index.cjs"
  },
  "scripts": {
    "build": "pnpm run build:util; pnpm run build:declarations",
    "build:util": "tsup src/index.ts --format esm,cjs",
    "build:declarations": "tsc --project tsconfig.declarations.json",
    "cmd:quick-test": "ts-node src/index.ts --debug",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepublishOnly": "pnpm build",
    "release": "release-it",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "author": "Jeff Wainwright <yowainwright@gmail.com> (https://jeffry.in)",
  "license": "MIT",
  "dependencies": {
    "commander": "^9.4.0",
    "compare-versions": "^4.1.3",
    "cosmiconfig": "^7.0.1",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.7.10",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "release-it": "^15.3.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "tsup": "^6.2.2",
    "tsutils": "^3.21.0",
    "typescript": "^4.7.4",
    "vitest": "^0.22.1"
  }
}
